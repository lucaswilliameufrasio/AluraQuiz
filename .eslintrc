{
    "settings": {
        "react": {
            "version": "detect"
        }
    },
    "extends": [
        "plugin:react/recommended",
        "standard"
    ],
    "plugins": [
        "react"
    ],
    "rules": {
        "no-redeclare": "off",
        "import/export": "off",
        "react/prop-types": "off",
        "react/jsx-uses-react": "error",
        "react/jsx-uses-vars": "error"
    }
}